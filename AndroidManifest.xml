<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ontimedeliv"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ontimedeliv.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ontimedeliv.NavigationActivity"
            android:label="@string/title_activity_navigation" >
        </activity>
        <activity
            android:name="com.example.ontimedeliv.CategoriesActivity"
            android:label="@string/title_activity_categories" >
        </activity>
        <activity
            android:name="com.example.ontimedeliv.BranchesActivity"
            android:label="@string/title_activity_branches" >
        </activity>
        <activity
            android:name="com.example.ontimedeliv.AddBranchActivity"
            android:label="@string/title_activity_add_branch" >
        </activity>
        <activity
            android:name="com.example.ontimedeliv.SelectionActivity"
            android:label="Select" >
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>